#include <iostream>
using namespace std;
int main() {
   
   int a,b;
   
   cin >> a >> b;
                      // Using Multiplication
   a = a + b;         // a = a * b;
   b = a - b;         // b = a / b;
   a = a - b;         // a = a / b;
   
   cout << a << " " << b;
   
    // Write C++ code here
   
    return 0;
}
